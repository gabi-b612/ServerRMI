<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.localchat.serverrmi.Controller">
    <ListView fx:id="userList" layoutX="50.0" layoutY="50.0" prefHeight="200.0" prefWidth="300.0"/>
    <TextField fx:id="nameField" layoutX="50.0" layoutY="270.0" promptText="Name"/>
    <TextField fx:id="loginField" layoutX="50.0" layoutY="310.0" promptText="Login"/>
    <TextField fx:id="passwordField" layoutX="50.0" layoutY="350.0" promptText="Password"/>
    <Button fx:id="addButton" layoutX="50.0" layoutY="400.0" text="Add User"/>
</AnchorPane>

